  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^4.3.0",
    "mocha": "^3.2.0",
    "nyc": "^11.1.0",
    "react": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10"
  }
